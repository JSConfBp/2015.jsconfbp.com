<!DOCTYPE html>
<html lang="en">
	<head>
		<meta charset="utf-8" />
		<meta http-equiv="X-UA-Compatible" content="IE=edge" />
		<meta name="viewport" content="width=device-width, initial-scale=1" />

		<title><%= @getPreparedTitle() %></title>
		<meta name="description" content="<%= @getPreparedDescription() %>" />
		<meta name="keywords" content="<%= @getPreparedKeywords() %>" />

        <meta property="og:url" content="http://jsconfbp.com" />
        <meta property="og:title" content="<%= @getPreparedTitle() %>" />
        <meta property="og:image" content="http://jsconfbp.com/images/og_image.jpg" />
		<meta property="og:description" content="JSConf Budapest is a JSConf family member, 2-day, non-profit, for-the-community by-the-community conference about JavaScript in Budapest, Hungary." />

		<%- @getBlock('meta').toHTML() %>

		<link href='//fonts.googleapis.com/css?family=Lato:300,400,700,900' rel='stylesheet' type='text/css'>
		<%- @getBlock('styles').add(@getGruntedStyles()).toHTML() %>

		<link rel="shortcut icon" href="./favicon.ico">
	</head>
	<body>
		<div id="header">

			<%- @partial('logo') %>
			<%- @partial('title') %>

            <div id="social">
                <a class="twitter" href="https://twitter.com/jsconfbp">@jsconfbp on Twitter</a>
                <a class="facebook" href="https://www.facebook.com/pages/JSConf-Budapest/383505985164517">JSConf Budapest on Facebook</a>
            </div>

			<div id="navigation">
				<ul>
					<li>
						<a href="/#"><span class="circle"></span></a>
					</li>
					<li>
						<a href="/#speakers">Speakers</a>
					</li>
					<li>
                    	<a href="/#venue">Venue</a>
                    </li>
					<li>
						<a href="/#sponsors">Sponsors</a>
					</li>
					<li>
						<a href="<%- @getPath('call-for-presentations.html','/') %>">Call For Presentations</a>
					</li>
					<li>
						<a href="<%- @getPath('budapest-guide.html','/') %>">Budapest Guide</a>
					</li>
					<li>
						<a href="<%- @getPath('about.html','/') %>">About</a>
					</li>
				</ul>
			</div>

			<div class="bg-layer"></div>
		</div>

		<div id="content">
			<%- @content %>
		</div>

		<%- @partial('footer') %>

		<script src="http://code.jquery.com/jquery-1.11.2.min.js"></script>
		<!-- DocPad Scripts + Our Own -->
		<%- @getBlock('scripts').add(@getGruntedScripts()).toHTML() %>

		 <script>
              (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
              (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
              m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
              })(window,document,'script','//www.google-analytics.com/analytics.js','ga');
              ga('create', 'UA-58489830-1', 'auto');
              ga('send', 'pageview');
         </script>
	</body>
</html>
